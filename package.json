{
   "name": "talkarr",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only --no-notify --project backend/tsconfig.json -r tsconfig-paths/register --watch backend --ignore-watch .next --ignore-watch src backend/index.ts",
      "build:next": "next build",
      "build": "npm run build:next",
      "start": "ts-node src/index.ts",
      "start:prod": "prisma migrate deploy && npm run start",
      "lint": "next lint",
      "typecheck:web": "tsc --noEmit",
      "typecheck:backend": "tsc --noEmit --project backend/tsconfig.json",
      "typecheck": "npm run typecheck:web && npm run typecheck:backend",
      "migration:init": "prisma migrate dev --name init",
      "dev:db": "docker compose -f docker-compose.dev.yml up -d",
      "db:reset": "docker compose -f docker-compose.dev.yml down -v && docker compose -f docker-compose.dev.yml up -d",
      "typeorm": "typeorm-ts-node-commonjs",
      "generate:openapi-backend": "openapi-typescript ./backend.json --output backend/generated/schema.ts",
      "generate": "npm run generate:openapi-backend"
   },
   "dependencies": {
      "@emotion/cache": "^11.14.0",
      "@emotion/react": "^11.14.0",
      "@emotion/styled": "^11.14.0",
      "@mui/icons-material": "^6.3.0",
      "@mui/material": "^6.3.0",
      "@mui/material-nextjs": "^6.3.0",
      "@prisma/client": "6.1.0",
      "@uidotdev/usehooks": "^2.4.1",
      "cookie-parser": "^1.4.7",
      "deepmerge": "^4.3.1",
      "express": "^4.21.2",
      "express-winston": "^4.2.0",
      "markdown-to-jsx": "^7.7.2",
      "moment": "^2.30.1",
      "next": "15.1.3",
      "openapi-fetch": "^0.13.3",
      "pg": "^8.4.0",
      "pretty-bytes": "^6.1.1",
      "prisma": "^6.1.0",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "react-markdown": "^9.0.1",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^5.0.1",
      "ts-node": "^10.9.2",
      "type-fest": "^4.31.0",
      "winston": "^3.17.0",
      "winston-daily-rotate-file": "^5.0.0",
      "zustand": "^5.0.2"
   },
   "devDependencies": {
      "@eslint/compat": "^1.2.4",
      "@eslint/eslintrc": "^3",
      "@eslint/js": "^9.17.0",
      "@types/cookie-parser": "^1.4.8",
      "@types/express": "^5.0.0",
      "@types/node": "^20",
      "@types/react": "^19",
      "@types/react-dom": "^19",
      "@types/swagger-ui-express": "^4.1.7",
      "eslint": "^9",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-airbnb-typescript": "^18.0.0",
      "eslint-config-next": "15.1.3",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-deprecation": "^3.0.0",
      "eslint-plugin-import": "^2.31.0",
      "eslint-plugin-mui-path-imports": "^0.0.15",
      "eslint-plugin-no-relative-import-paths": "^1.5.5",
      "eslint-plugin-prettier": "^5.2.1",
      "eslint-plugin-simple-import-sort": "^12.1.1",
      "openapi-typescript": "^7.4.4",
      "openapi-typescript-helpers": "^0.0.15",
      "prettier": "^3.4.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5",
      "typescript-eslint": "^8.18.2"
   },
   "prisma": {
      "seed": "ts-node --transpile-only prisma/seed.ts"
   }
}

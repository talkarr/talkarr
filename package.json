{
    "name": "talkarr",
    "version": "0.1.0",
    "license": "AGPL-3.0-only",
    "private": true,
    "scripts": {
        "dev:tsnd": "ts-node-dev --respawn --transpile-only --no-notify --project backend/tsconfig.json -r tsconfig-paths/register --watch backend --ignore-watch .next --ignore-watch src backend/index.ts",
        "dev:nodemon": "nodemon --exec ts-node --project backend/tsconfig.json -r tsconfig-paths/register backend/index.ts",
        "dev": "cross-env NODE_OPTIONS='--disable-warning=DEP0174' npm run dev:nodemon",
        "build:next": "next build",
        "build": "npm run build:next",
        "start": "cross-env NODE_OPTIONS='--disable-warning=DEP0174' NODE_ENV=production ts-node --project backend/tsconfig.json -r tsconfig-paths/register backend/index.ts",
        "start:prod": "npm run migration:deploy && npm run start",
        "lint:frontend": "next lint",
        "lint:frontend:fix": "next lint --fix",
        "lint:backend": "eslint backend",
        "lint:backend:fix": "eslint backend --fix",
        "lint:e2e": "eslint e2e",
        "lint:e2e:fix": "eslint e2e --fix",
        "lint": "npm run lint:frontend && npm run lint:backend && npm run lint:e2e",
        "typecheck:web": "tsc --noEmit",
        "typecheck:backend": "tsc --noEmit --project backend/tsconfig.json",
        "typecheck": "npm run typecheck:web && npm run typecheck:backend",
        "migration:init": "prisma migrate dev --name init && ts-node --project backend/tsconfig.json -r tsconfig-paths/register prisma/seed.ts",
        "migration:deploy": "node_modules/.bin/dotenv -e .env prisma migrate deploy",
        "dev:db": "docker compose -f docker-compose.dev.yml up -d db",
        "dev:redis": "docker compose -f docker-compose.dev.yml up -d redis",
        "db:reset": "docker compose -f docker-compose.dev.yml down -v && docker compose -f docker-compose.dev.yml up -d db redis && NODE_ENV=development APPLY_PRESET=1 ts-node --project backend/tsconfig.json -r tsconfig-paths/register prisma/seed.ts",
        "redis:reset": "docker compose -f docker-compose.dev.yml down -v redis && docker compose -f docker-compose.dev.yml up -d redis",
        "typeorm": "typeorm-ts-node-commonjs",
        "generate:openapi-backend": "openapi-typescript ./backend.json --output backend/generated/schema.ts",
        "generate": "npm run generate:openapi-backend",
        "test:backend": "bash -c 'pushd backend && jest && popd'",
        "test:e2e:ci": "CI=true npm run test:e2e",
        "test:e2e": "playwright test",
        "prepare": "ts-patch install && typia patch",
        "postinstall": "patch-package"
    },
    "dependencies": {
        "@emotion/cache": "^11.14.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.1.1",
        "@mui/material": "^7.1.1",
        "@mui/material-nextjs": "^7.1.1",
        "@prisma/client": "6.8.2",
        "@ryoppippi/unplugin-typia": "^2.6.4",
        "@uidotdev/usehooks": "^2.4.1",
        "bull": "^4.16.5",
        "cookie-parser": "^1.4.7",
        "cross-env": "^7.0.3",
        "deepmerge": "^4.3.1",
        "dotenv": "^16.5.0",
        "dotenv-cli": "^8.0.0",
        "express": "^5.1.0",
        "express-winston": "^4.2.0",
        "iso-639-1": "^3.1.4",
        "jest": "^29.7.0",
        "markdown-to-jsx": "^7.7.6",
        "memory-cache": "^0.2.0",
        "mime-types": "^3.0.1",
        "moment": "^2.30.1",
        "next": "15.3.3",
        "notistack": "^3.0.2",
        "openapi-fetch": "^0.14.0",
        "patch-package": "^8.0.0",
        "pg": "^8.16.0",
        "pretty-bytes": "^7.0.0",
        "prisma": "^6.8.2",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-markdown": "^10.1.0",
        "reflect-metadata": "^0.2.2",
        "sharp": "^0.34.2",
        "simple-git": "^3.27.0",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.3.0",
        "tsconfig-paths": "^4.2.0",
        "type-fest": "^4.40.1",
        "typescript": "~5.8.3",
        "typia": "^9.3.1",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0",
        "youtube-dl-exec": "3.0.22",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.9",
        "@eslint/eslintrc": "^3",
        "@eslint/js": "^9.28.0",
        "@microsoft/eslint-formatter-sarif": "^3.1.0",
        "@playwright/test": "^1.52.0",
        "@types/bull": "^4.10.4",
        "@types/cookie-parser": "^1.4.8",
        "@types/express": "^5.0.2",
        "@types/jest": "^29.5.14",
        "@types/memory-cache": "^0.2.6",
        "@types/mime-types": "^3.0.0",
        "@types/node": "^22",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/swagger-ui": "^5.21.1",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/wait-on": "^5.3.4",
        "eslint": "^9",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-next": "15.3.3",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-mui-path-imports": "^0.0.15",
        "eslint-plugin-no-relative-import-paths": "^1.6.1",
        "eslint-plugin-playwright": "^2.2.0",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "nodemon": "^3.1.10",
        "openapi-typescript": "^7.8.0",
        "openapi-typescript-helpers": "^0.0.15",
        "prettier": "^3.5.3",
        "ts-jest": "^29.3.4",
        "ts-node-dev": "^2.0.0",
        "typescript-eslint": "^8.33.0",
        "wait-on": "^8.0.3"
    },
    "engines": {
        "node": ">= 18.18"
    },
    "nodemonConfig": {
        "watch": [
            "./backend",
            "next.config.ts"
        ],
        "ignore": [
            ".next",
            "src"
        ],
        "ext": "ts,json"
    }
}
